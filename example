import sys
import os
import subprocess
from common import logger, sgcloud
from contextlib import redirect_stdout

SCRIPT_DIR = os.path.dirname(os.path.abspath(__file__))  
NOTIFY_SCRIPT = os.path.join(SCRIPT_DIR, "../scripts/notify/notify_image_region_mismatch.sh") 

def main():
    if len(sys.argv) < 5:
        print("Usage: compare_latest_ocs_images.py <LATEST_OS_IMAGE_paris> <LATEST_OS_IMAGE_north> <LIST_OS_IMAGE_paris> <LIST_OS_IMAGE_north>")
        sys.exit(1)

    latest_os_paris = sys.argv[1]
    latest_os_north = sys.argv[2]
    
    list_os_paris = eval(sys.argv[3])  # Convert string representation of list to actual list
    list_os_north = eval(sys.argv[4])  # Convert string representation of list to actual list

    # Step 1: Compare the first two input values
    if latest_os_paris == latest_os_north:
        print(f"Images are equal: {latest_os_paris}")
        sys.exit(0)  # Exit successfully since images are the same

    # Step 2: Compare the list values to find the latest shared image
    common_images = sorted(set(list_os_paris) & set(list_os_north), reverse=True)

    if common_images:
        latest_common_image = common_images[0]  # Get the latest (highest) image
        print(f"Mismatch detected. Latest shared image: {latest_common_image}")
    else:
        print("No shared image found between the two regions.")

if __name__ == "__main__":
    main()
