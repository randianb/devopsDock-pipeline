import groovy.json.JsonSlurper

// Ensure REGION is a proper JSON string
def REGIONS = params.REGION.trim() 

// Remove extra single quotes if present
if (REGIONS.startsWith("'") && REGIONS.endsWith("'")) {
    REGIONS = REGIONS[1..-2]
}

def jsonSlurper = new JsonSlurper()
def regionList = jsonSlurper.parseText(REGIONS) // Converts to a List

println(regionList) // Output: [paris, north]
println(regionList[0]) // Output: paris (first element)