import sys
from common import logger, sgcloud
import os
from contextlib import redirect_stdout

SCRIPT_DIR = os.path.dirname(os.path.abspath(__file__))  
NOTIFY_SCRIPT = os.path.join(SCRIPT_DIR, "../scripts/notify/notify_image_region_mismatch.sh") 


def compare_os_image(region):
    sg_cloud_client = sgcloud.Client(os.environ['ACCOUNT_ID'],os.environ['CLIENT_ID'],os.environ['CLIENT_SECRET'])
    # redirect stdout to /dev/null
    with open(os.devnull,'w') as devnull:
        with redirect_stdout(devnull):        
            compare_images = sg_cloud_client.compare_image()
    # send the image name via stdout
    print(compare_images)
    
def main():
    region = sys.argv[1]
    regions_images = compare_os_image(region)

        # Check if there are mismatched images
    unique_images = set(regions_images.values())

    if len(unique_images) > 1:
        mismatch_output = "\n".join(f"{region} = {image}" for region, image in regions_images.items())
        
        # Trigger notification script if there is a mismatch
        subprocess.run(["sh", NOTIFY_SCRIPT, server_name, mismatch_output], check=True)
    
if __name__ == "__main__":
    main()

