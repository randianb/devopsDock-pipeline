yum groupinstall 'Development Tools'
yum install python3-devel postgresql-devel

wget https://objs3parstd01.fr.world.socgen/xsf-af567-prd-artifacts/public/python/psycopg2-2.9.10.tar.gz
tar -xzf psycopg2-2.9.10.tar.gz
cd psycopg2-2.9.10

python3 setup.py build
python3 setup.py install

export PYTHONPATH=/usr/local/lib64/python3.9/site-packages/:$PYTHONPATH
pip list 
